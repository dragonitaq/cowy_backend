{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "post",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true
    },
    "content": {
      "type": "richtext",
      "required": true
    },
    "private": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "postImageCover": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images"
      ],
      "plugin": "upload",
      "required": false
    },
    "users_permissions_user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "blogs"
    },
    "slug": {
      "type": "uid",
      "targetField": "title",
      "required": true
    },
    "tag": {
      "type": "json"
    }
  }
}
